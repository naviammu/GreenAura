<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="12dp" />
            <gradient
                android:startColor="#A8E6CF"
            android:endColor="#56C596"
            android:angle="90" />
            <padding
                android:left="16dp"
                android:top="8dp"
                android:right="16dp"
                android:bottom="8dp" />
        </shape>
    </item>
</layer-list>
